{
  "name": "slackin",
  "version": "0.14.0",
  "description": "Public Slack organizations made easy",
  "repository": "rauchg/slackin",
  "main": "dist/index",
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "args": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "email-regex": "^3.0.0",
    "express": "^4.17.1",
    "hostenv": "^1.0.6",
    "socket.io": "^2.2.0",
    "superagent": "^5.0.5",
    "vd": "0.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "cpy-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">= 8"
  },
  "bin": {
    "slackin": "./bin/slackin"
  },
  "scripts": {
    "copy-assets": "cpy lib/assets/* dist/assets",
    "build-js": "babel lib/*.js -d dist/",
    "test": "mocha && eslint lib/**",
    "postinstall": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run build-js && npm run copy-assets",
    "start": "chmod +x bin/slackin && ./bin/slackin"
  },
  "now": {
    "type": "npm",
    "files": [
      "bin",
      "lib"
    ],
    "env": [
      "SLACK_API_TOKEN",
      "SLACK_SUBDOMAIN",
      "GOOGLE_CAPTCHA_SECRET",
      "GOOGLE_CAPTCHA_SITEKEY"
    ]
  }
}
