{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  },
  "overrides": [
    {
      "files": "lib/assets/*.js",
      "env": {
        "browser": true
      }
    },
    {
      "files": "lib/*.js",
      "env": {
        "node": true
      }
    }
  ]
}
